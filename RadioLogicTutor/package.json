{
  "name": "radiologic-tutor",
  "version": "1.0.1",
  "description": "RadioLogicTutor",
  "keywords": [
    "radiology",
    "DICOM",
    "cornerstone",
    "medical",
    "imaging"
  ],
  "author": "Marco Barnig",
  "homepage": "http://www.web3.lu",
  "license": "MIT",
  "main": "./dist/radiologictutor.min.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbarnig/radiologictutor.git"
  },
  "scripts": {
    "eslint": "eslint -c .eslintrc.js src",
    "eslint-quiet": "eslint -c .eslintrc.js --quiet src",
    "eslint-fix": "eslint -c .eslintrc.js --fix src",
    "clean": "npm run clean:dist && npm run clean:coverage",
    "clean:dist": "shx rm -rf dist",
    "clean:docs": "shx rm -rf documentation",
    "clean:coverage": "shx rm -rf coverage",
    "doc": "npm run doc:generate && opn documentation/index.html",
    "doc:generate": "npm run clean:docs && jsdoc -c .jsdocrc",
    "build": "npm run clean:dist && npm run build:es6",
    "build:es6": "npm run test && npm run version && npm run webpack && npm run doc:generate",
    "start": "npm run webpack",
    "webpack": "npm run webpack:prod && npm run webpack:dev",
    "webpack:dev": "webpack --progress --config ./config/webpack/webpack-dev",
    "webpack:prod": "webpack --progress --config ./config/webpack/webpack-prod",
    "uglify:codecs": "uglifyjs --comments /^/\\*!/ --stats -o ./dist/radiologicteacher.min.js"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "concat": "^1.0.3",
    "coveralls": "^2.13.1",
    "docdash": "^0.4.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.7.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsdoc": "^3.4.3",
    "lodash": "4.17.19",
    "mocha": "^3.2.0",
    "opn-cli": "^3.1.0",
    "shx": "^0.2.2",
    "uglify-js": "^3.0.7",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "jquery-mobile": "^1.5.0-alpha.1",
    "jszip": "^3.1.3",
    "inobounce": "^0.1.5",
    "drag-drop-touch-polyfill": "^1.0.0",
    "hammerjs": "^2.0.8",
    "jqplot": "^1.0.9",
    "jqplot.donutRenderer": "^1.0.9",
    "cornerstone-core": "^0.12.2",
    "cornerstone-dicom-parser-utf8": "^1.7.54",
    "cornerstone-tools": "^0.9.0",
    "cornerstone-math": "^0.1.6",
    "cornerstone-wado-image-loader": "^0.14.7",
    "cornerstone-archive-image-loader": "^0.14.71"
  }
}
